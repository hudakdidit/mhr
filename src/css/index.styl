ease-out-quart = cubic-bezier(0.165, 0.84, 0.44, 1)

tablet = 30em
above($width)
  @media (min-width: $width)
    {block}

@import './modules/*'

*
  margin 0
  padding 0
  line-height 1

html, body
  height 100%

body
  font-family 'Roboto', sans-serif
  box-sizing border-box

  *
    box-sizing inherit
